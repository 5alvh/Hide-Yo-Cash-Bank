spring.security.user.password=${SECURITY_USER_PASSWORD:admin}

logging.pattern.console=${LOG_PATTERN_CONSOLE:%green(%d{HH:mm:ss:SSS}) %yellow([%thread]) %blue(%-5level) %cyan(%logger{15}) - %msg%n}



