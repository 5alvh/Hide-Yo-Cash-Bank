package com.Salah.miBanco;

public class Demo {

    private String name;

    public Demo(String name) {
        this.name = name;
    }
    public String getName() {
        return name;
    }

    public static void main(String[] args) {
        Demo demo = new Demo("Salah");
        System.out.println(demo.getName());
    }
}
